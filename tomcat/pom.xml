    <artifactId>beangle-sas-parent</artifactId>
    <version>0.7.2-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>beangle-sas-tomcat</artifactId>
  <packaging>jar</packaging>
